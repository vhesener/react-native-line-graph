{
  "name": "react-native-line-graph",
  "author": "Vinnie Hesener",
  "description": "React Native line graph using d3",
  "version": "0.0.1",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/vhesener/react-native-line-graph.git"
  },
  "main": "index",
  "scripts": {
